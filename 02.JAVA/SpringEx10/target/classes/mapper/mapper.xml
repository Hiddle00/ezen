<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<!-- Namespace를 통해 JAVA 클래스와 Annotation클래스와 동기 -->
<mapper namespace="com.ezen.data">

	<select id="one" parameterType="java.lang.String"
			resultType="com.ezen.vo.DataVO">
		select * from data
		where no = #{no}
	</select>

	<select id="list" resultType="com.ezen.vo.DataVO">
		select * from data
	</select>
	
</mapper>
